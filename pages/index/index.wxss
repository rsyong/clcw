.user_img{
  width: 110rpx;
  height: 80rpx;
  background: #fff;
  justify-content: flex-end;
  border-top-right-radius: 50rpx;
  border-bottom-right-radius: 50rpx;
  box-shadow: 0 0 8rpx 4rpx #EBEBEB;
}
.user_img image{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.fuwu{background: #fff;margin-top: 60rpx;box-shadow: 0 -1rpx 8rpx 4rpx #EBEBEB;}
.fuwu_list{
  height: 100rpx;
  border-bottom: 1px solid #F9F9F9;
}
.fuwu_list:last-child{
  border-bottom: 1px solid #EAEAEA;
}
.fuwu_list_logo{
  width: 100rpx;
}

navigator{font-size: 12px;}
